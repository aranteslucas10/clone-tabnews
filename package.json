{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Aprimorando meus conhecimentos com a implementação do https://www.tabnews.com.br feita no https://curso.dev",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test:watch": "jest --watchAll",
    "compose:local:up": "docker compose -f infra/compose.yaml --env-file .env.local up -d",
    "compose:local:ps": "docker compose -f infra/compose.yaml --env-file .env.local ps",
    "compose:local:recreate": "docker compose -f infra/compose.yaml --env-file .env.local up -d --force-recreate",
    "compose:local:down": "docker compose -f infra/compose.yaml --env-file .env.local down"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.0.2"
  }
}
